{
  "name": "es6-timed-map",
  "version": "0.2.0",
  "description": "An es6-map-like utility class with time based functions and support",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint ./src ./tests",
    "test": "jest",
    "test:coverage": "jest --coverage && make-coverage-badge",
    "build": "tsc",
    "clean": "rimraf ./lib",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradtaniguchi/es6-timed-map.git"
  },
  "bugs": {
    "url": "https://github.com/bradtaniguchi/es6-timed-map/issues"
  },
  "homepage": "https://github.com/bradtaniguchi/es6-timed-map#readme",
  "keywords": [
    "es6",
    "map",
    "timed",
    "cache"
  ],
  "author": "Brad Taniguchi",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.2",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  },
  "dependencies": {}
}
