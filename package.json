  },
  "dependencies": {},
  "source": "src/index.ts",
  "exports": "./lib/es6-timed-map.modern.js",
  "main": "./lib/es6-timed-map.csj",
  "module": "./lib/es6-timed-map.module.js",
  "unpkg": "./lib/es6-timed-map.umd.js",
  "types": "./lib/es6-timed-map.d.ts"
